     if line =~ peg"\s* {\w+} \s* '=' \s* {\w+}":
       # matches a key=value pair:
       echo("Key: ", matches[0])
       echo("Value: ", matches[1])
     elif line =~ peg"\s*{'#'.*}":
       # matches a comment
